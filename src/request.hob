import std;
import io;
import result;
import list;
import "common.hob" as common;

use io.fd.Fd;
use list.List;
use result.{ NoneResult, Result };
use common.{ ProtocolVersion, Header };

public type Request = struct {
    public method: string,
    public path: string,
    public query: string,
    public protocolVersion: ProtocolVersion,
    public headers: List.<Header>,
};

public fun Request.free(self) {
    self.headers.free();
}

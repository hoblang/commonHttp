import std;
import io;
import result;
import list;
import "common.hob" as common;

use io.fd.Fd;
use list.List;
use result.{ NoneResult, Result };

public type Header = struct {
    public name: string,
    public value: string,
};

public type Request = struct {
    public method: string,
    public path: string,
    public query: string,
    public protocolVersion: common.ProtocolVersion,
    public headers: List.<Header>,
};

public fun Request.free(self) {
    self.headers.free();
}
